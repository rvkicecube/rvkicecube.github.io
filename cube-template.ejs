```{=html}
<% 
const confirmedCubes = items.filter(cube => cube.category === 'confirmed');
const otherCubes = items.filter(cube => cube.category !== 'confirmed');
%>

<% if (confirmedCubes.length > 0) { %>
<h2>Confirmed Event Cubes</h2>
<div class="cubes-grid">
<% for (const cube of confirmedCubes) { %>
  <div class="cube-card">
    <% if (cube.image) { %>
    <div class="cube-image">
      <img src="<%= cube.image %>" alt="<%= cube.name %>" class="cube-art">
    </div>
    <% } %>
    
    <div class="cube-header">
      <div class="cube-title-section">
        <h3 class="cube-name"><%= cube.name %></h3>
        <% if (cube.creator) { %>
        <p class="cube-creator">Created by <strong><%= cube.creator %></strong></p>
        <% } %>
      </div>
      <div class="cube-badges">
        <span class="badge power-level <%= cube.power_level.toLowerCase() %>"><%= cube.power_level %> Power</span>
        <% if (cube.status) { %>
        <span class="badge status <%= cube.status.toLowerCase() %>"><%= cube.status %></span>
        <% } %>
      </div>
    </div>
    
    <div class="cube-description">
      <p><%= cube.description %></p>
    </div>
    
    <div class="cube-details">
      <div class="detail-row">
        <span class="detail-label">📦 Cards:</span>
        <span class="detail-value"><%= cube.card_count %></span>
      </div>
      <div class="detail-row">
        <span class="detail-label">🎭 Format:</span>
        <span class="detail-value"><%= cube.format %></span>
      </div>
      <% if (cube.first_used) { %>
      <div class="detail-row">
        <span class="detail-label">🗓️ First Used:</span>
        <span class="detail-value"><%= new Date(cube.first_used).toLocaleDateString() %></span>
      </div>
      <% } %>
    </div>
    
    <% if (cube.archetypes && cube.archetypes.length > 0) { %>
    <div class="cube-archetypes">
      <h4>🎯 Supported Archetypes</h4>
      <ul class="archetype-list">
        <% for (const archetype of cube.archetypes) { %>
        <li><%= archetype %></li>
        <% } %>
      </ul>
    </div>
    <% } %>
    
    <% if (cube.key_features && cube.key_features.length > 0) { %>
    <div class="cube-features">
      <h4>✨ Key Features</h4>
      <ul class="feature-list">
        <% for (const feature of cube.key_features) { %>
        <li><%= feature %></li>
        <% } %>
      </ul>
    </div>
    <% } %>
    
    <% if (cube.notes) { %>
    <div class="cube-notes">
      <p class="notes-text"><em><%= cube.notes %></em></p>
    </div>
    <% } %>
    
    <% if (cube.url) { %>
    <div class="cube-actions">
      <a href="<%= cube.url %>" target="_blank" rel="noopener noreferrer" class="cube-link">
        View on CubeCobra →
      </a>
    </div>
    <% } %>
  </div>
<% } %>
</div>
<% } %>

<% if (otherCubes.length > 0) { %>
<h2>Other Available Cubes</h2>
<div class="cubes-grid">
<% for (const cube of otherCubes) { %>
  <div class="cube-card">
    <% if (cube.image) { %>
    <div class="cube-image">
      <img src="<%= cube.image %>" alt="<%= cube.name %>" class="cube-art">
    </div>
    <% } %>
    
    <div class="cube-header">
      <div class="cube-title-section">
        <h3 class="cube-name"><%= cube.name %></h3>
        <% if (cube.creator) { %>
        <p class="cube-creator">Created by <strong><%= cube.creator %></strong></p>
        <% } %>
      </div>
      <div class="cube-badges">
        <span class="badge power-level <%= cube.power_level.toLowerCase() %>"><%= cube.power_level %> Power</span>
        <% if (cube.status) { %>
        <span class="badge status <%= cube.status.toLowerCase() %>"><%= cube.status %></span>
        <% } %>
      </div>
    </div>
    
    <div class="cube-description">
      <p><%= cube.description %></p>
    </div>
    
    <div class="cube-details">
      <div class="detail-row">
        <span class="detail-label">📦 Cards:</span>
        <span class="detail-value"><%= cube.card_count %></span>
      </div>
      <div class="detail-row">
        <span class="detail-label">🎭 Format:</span>
        <span class="detail-value"><%= cube.format %></span>
      </div>
      <% if (cube.first_used) { %>
      <div class="detail-row">
        <span class="detail-label">🗓️ First Used:</span>
        <span class="detail-value"><%= new Date(cube.first_used).toLocaleDateString() %></span>
      </div>
      <% } %>
    </div>
    
    <% if (cube.archetypes && cube.archetypes.length > 0) { %>
    <div class="cube-archetypes">
      <h4>🎯 Supported Archetypes</h4>
      <ul class="archetype-list">
        <% for (const archetype of cube.archetypes) { %>
        <li><%= archetype %></li>
        <% } %>
      </ul>
    </div>
    <% } %>
    
    <% if (cube.key_features && cube.key_features.length > 0) { %>
    <div class="cube-features">
      <h4>✨ Key Features</h4>
      <ul class="feature-list">
        <% for (const feature of cube.key_features) { %>
        <li><%= feature %></li>
        <% } %>
      </ul>
    </div>
    <% } %>
    
    <% if (cube.notes) { %>
    <div class="cube-notes">
      <p class="notes-text"><em><%= cube.notes %></em></p>
    </div>
    <% } %>
    
    <% if (cube.url) { %>
    <div class="cube-actions">
      <a href="<%= cube.url %>" target="_blank" rel="noopener noreferrer" class="cube-link">
        View on CubeCobra →
      </a>
    </div>
    <% } %>
  </div>
<% } %>
</div>
<% } %>
```